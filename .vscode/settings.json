{
  "prettier.configPath": "./scripts/.prettierrc",
  "markdownlint.config": {
    "extends": "./scripts/.markdownlintrc"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "dictCompletion.externalUserDictFiles": [],
  "dictCompletion.leastNumOfChars": 4,
  "spellright.language": [
    "English (British)"
  ],
  "spellright.documentTypes": [
    "latex",
    "plaintext",
    "markdown"
  ]
}